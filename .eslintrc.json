{
  "env": {
    "es6": true
  },
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "expo"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "linebreak-style": ["error", "unix"],
    "no-unused-vars": 0
  }
}
